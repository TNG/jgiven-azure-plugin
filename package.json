    "package": "tfx extension create",
    "clean": "rimraf ./dist && rimraf ./*.vsix"
  },
  "author": "Andru-Lucian Stefanescu",
  "contributors": [
    {
      "name": "Christian Oertel"
    },
    {
      "name": "Kristof Karhan"
    }
  ],
  "devDependencies": {
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "tfx-cli": "^0.9.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "azure-pipelines-task-lib": "^3.1.9",
    "@types/jquery": "^3.5.6",
    "vss-web-extension-sdk": "^5.141.0"
  }
}
